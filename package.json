{
	"name": "@esm2cjs/formdata-polyfill",
	"version": "4.0.10",
	"description": "HTML5 `FormData` for Browsers and Node.. This is a fork of jimmywarting/FormData, but with CommonJS support.",
	"main": "cjs/index.js",
	"scripts": {},
	"repository": "esm2cjs/formdata-polyfill",
	"files": [
		"cjs/",
		"esm/"
	],
	"engines": {
		"node": ">=12.20.0"
	},
	"keywords": [
		"formdata",
		"fetch",
		"node-fetch",
		"html5",
		"browser",
		"polyfill"
	],
	"author": {
		"name": "Dominic Griesel",
		"email": "d.griesel@gmx.net"
	},
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/jimmywarting/FormData/issues"
	},
	"homepage": "https://github.com/jimmywarting/FormData#readme",
	"dependencies": {
		"@esm2cjs/fetch-blob": "^3.1.2"
	},
	"devDependencies": {
		"@types/node": "^16.7.10"
	},
	"publishConfig": {
		"access": "public"
	},
	"funding": "https://github.com/sponsors/AlCalzone",
	"module": "esm/index.js",
	"exports": {
		".": {
			"import": "./esm/index.js",
			"require": "./cjs/index.js"
		},
		"./package.json": "./package.json"
	},
	"types": "esm/index.d.ts",
	"typesVersions": {
		"*": {
			"esm/index.d.ts": [
				"esm/index.d.ts"
			],
			"cjs/index.d.ts": [
				"esm/index.d.ts"
			],
			"*": [
				"esm/*"
			]
		}
	}
}
